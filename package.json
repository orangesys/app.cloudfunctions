{
  "name": "orangesys-functions",
  "version": "0.1.0",
  "description": "orangesys.io server side",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src/ test/",
    "flow": "flow",
    "check": "npm run lint && npm run flow",
    "test:unit": "mocha test/unit/",
    "test:integration": "env $(cat .env | xargs) mocha test/integration/",
    "test": "npm run test:unit && npm run test:integration",
    "build": "rm -fr functions/* && babel src --out-dir functions && cp -f package.json functions/package.json && (cd functions/ && npm install --production --silent)",
    "deploy": "npm run build && firebase deploy --only functions"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.15.2",
    "babel-polyfill": "^6.23.0",
    "babel-runtime": "^6.23.0",
    "cors": "^2.8.1",
    "es6-promisify": "^5.0.0",
    "express": "^4.15.2",
    "firebase-admin": "^4.1.3",
    "firebase-functions": "^0.5.2",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.16.4",
    "moment": "^2.15.1",
    "node-mailjet": "^3.0.6",
    "stripe": "~4.12.0"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-espower": "^2.3.2",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-es2016": "^6.22.0",
    "babel-preset-es2017": "^6.22.0",
    "babel-preset-power-assert": "^1.0.0",
    "babel-register": "^6.23.0",
    "eslint": "^3.16.0",
    "eslint-config-airbnb-base": "^11.1.0",
    "eslint-plugin-flow-vars": "^0.5.0",
    "eslint-plugin-flowtype": "^2.30.0",
    "eslint-plugin-import": "^2.2.0",
    "firebase-tools": "^3.9.1",
    "flow-bin": "^0.41.0",
    "flow-typed": "^2.1.0",
    "mocha": "^3.2.0",
    "power-assert": "^1.4.2",
    "proxyquire": "^1.7.11",
    "sinon": "^2.0.0",
    "testdouble": "^2.0.3"
  }
}
